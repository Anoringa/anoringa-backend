SERVER_PORT=3000
# Example Port String:-  
# 3000

SERVER_ADDRESS=0.0.0.0
# Example Address String:-  
# 127.0.0.1
# 0.0.0.0

MONGODB_URL=YourConnectionString
# Example Connection String:-  
# mongodb://127.0.0.1:27017/rest-api-nodejs-mongodb
